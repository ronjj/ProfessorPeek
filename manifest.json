{
    "manifest_version": 3,
    "name": "ProfessorPeek",
    "version": "1.0",
    "description": "Add the RateMyProfessor rating and CUReviews rating to the course catalog",
    "icons": {
        "16": "images/pp-logo-16.png",
        "32": "images/pp-logo-32.png",
        "48": "images/pp-logo-48.png",
        "128": "images/pp-logo-128.png"
      },
        "content_scripts": [
          {
            "js": ["scripts/content.js"],
            "matches": [
              "https://developer.chrome.com/docs/extensions/*",
              "https://developer.chrome.com/docs/webstore/*",
              "https://amirbolous.com/*"
            ]
          }
        ]
  }

  